{
    "manifest_version": 2,
    "name": "GitGPT",
    "version": "1.0",
    "description": "Launch Python script from Firefox",
    "permissions": [
      "storage",
      "<all_urls>",
      "file://*/*",
      "http://localhost/"
    ],    
    "icons": {
      "96": "icons/icon.png"
    },
    "browser_action": {
      "default_icon": "icons/icon.png",
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      "style.css"
    ]
    
  }
  